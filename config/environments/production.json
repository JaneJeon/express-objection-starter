{
  "proxy": { "trust": true, "enforceHTTPS": { "trustProtoHeader": true } },
  "logger": {
    "options": { "level": "info", "prettyPrint": false },
    "serializers": { "req": ["id", "method", "url", "user"] }
  },
  "session": { "secret": null },
  "database": { "debug": false },
  "mail": {
    "smtp": {
      "service": "sendgrid",
      "auth": { "user": "apikey", "pass": null }
    },
    "defaults": { "from": "production@example.com" }
  }
}
