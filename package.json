{
  "name": "express-objection-starter",
  "version": "0.9.1",
  "description": "an opinionated, production-ready express/knex/objection starter",
  "keywords": [
    "express",
    "objection",
    "knex",
    "starter",
    "template",
    "boilerplate",
    "passport",
    "postgres",
    "algolia",
    "elasticsearch",
    "redis",
    "api",
    "backend",
    "jsonschema",
    "validation",
    "email",
    "template",
    "handlebars",
    "mjml",
    "access-control",
    "ACL",
    "RBAC",
    "roles",
    "policy",
    "nodemailer"
  ],
  "repository": "JaneJeon/express-objection-starter",
  "author": "Jane Jeon <JaneJeon9719@gmail.com>",
  "license": "MIT",
  "scripts": {
    "setup": "yarn build && yarn migrate",
    "start": "pm2 start pm2.config.js",
    "start:web": "pm2-runtime start pm2.config.js --only server",
    "start:worker": "pm2-runtime start pm2.config.js --only worker",
    "dev": "run-p dev:* watch",
    "dev:web": "nodemon server",
    "dev:worker": "wait-on http://localhost:1080 && nodemon worker",
    "dev:mail": "maildev",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "seed:make": "knex seed:make",
    "build": "run-s build:*",
    "build:yaml": "yaml2json config --save --recursive",
    "build:mjml": "node scripts/generate-templates",
    "build:hbs": "handlebars views/emails -e hbs -f views/emails/index.js",
    "watch": "run-p watch:*",
    "watch:yaml": "yarn build:yaml --watch",
    "watch:mjml": "nodemon -e mjml -x 'yarn build:mjml'",
    "watch:hbs": "nodemon -e hbs -x 'yarn build:hbs'",
    "pretest": "yarn setup",
    "test": "run-p dev:mail 'test:unit {@}' --race -- --",
    "test:watch": "run-p watch 'test --watch'",
    "test:unit": "wait-on http://localhost:1080 && jest",
    "coverage": "is-ci && codecov",
    "lint": "run-s 'lint:all:* {@}' -- --",
    "lint:all:eslint": "eslint --cache .",
    "lint:all:prettier": "prettier --check '**/*.{js,ts,html,css,json,md,yml,yaml}'",
    "lint:fix": "run-s lint:fix:*",
    "lint:fix:eslint": "yarn lint:all:eslint --fix",
    "lint:fix:prettier": "yarn lint:all:prettier --write",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:deploy": "surge docs/.vuepress/dist https://objection.surge.sh",
    "clean": "rimraf logs/*.log docs/.vuepress/dist reports coverage config/**/*.json views/**/html.hbs views/emails/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:fix",
      "pre-push": "yarn test",
      "post-merge": "yarn && yarn setup",
      "post-receive": "yarn --production && yarn setup"
    }
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true,
      "jest": true
    },
    "extends": [
      "plugin:prettier/recommended",
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2018
    }
  },
  "eslintIgnore": [
    "views/emails/index.js"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "proseWrap": "never"
  },
  "nodemonConfig": {
    "events": {
      "start": "cls || clear"
    },
    "ignore": [
      "*.test.js",
      "migrations/",
      "seeds/"
    ],
    "env": {
      "NODE_ENV": "development"
    }
  },
  "dependencies": {
    "bull": "^3.10.0",
    "connect-redis": "^3.4.2",
    "cors": "^2.8.5",
    "dayjs": "^1.8.14",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-query-boolean": "^2.0.0",
    "express-rate-limit": "^5.0.0",
    "express-request-id": "^1.4.1",
    "express-session": "^1.16.1",
    "express-ws": "^4.0.0",
    "glob": "^7.1.4",
    "got": "^9.6.0",
    "handlebars": "^4.1.2",
    "helmet": "^3.18.0",
    "http-assert": "^1.4.1",
    "husky": "^3.0.3",
    "ioredis": "^4.9.5",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.2",
    "lodash": "^4.17.11",
    "mailchecker": "^3.2.35",
    "mjml": "^4.4.1",
    "morgan": "^1.9.1",
    "ms": "^2.1.2",
    "mssql": "^5.1.0",
    "mysql": "^2.17.1",
    "mysql2": "^1.6.5",
    "nanoid": "^2.0.3",
    "nconf": "^0.10.0",
    "nodemailer": "^6.2.1",
    "normalize-email": "^1.1.1",
    "objection": "^1.6.9",
    "objection-authorize": "^0.5.3",
    "objection-db-errors": "^1.1.1",
    "objection-hashid": "^0.2.2",
    "objection-password": "^2.0.0",
    "objection-table-name": "^1.0.0",
    "objection-visibility": "^0.4.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.11.0",
    "pino": "^5.13.1",
    "pm2": "^3.5.1",
    "rate-limit-redis": "^1.6.0",
    "restana": "^3.3.1",
    "role-acl": "^3.3.5",
    "sqlite3": "^4.1.0",
    "ua-parser-js": "^0.7.20",
    "yamljs": "^0.3.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.1",
    "utf-8-validate": "^5.0.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.17",
    "codecov": "^3.5.0",
    "cross-env": "^5.2.0",
    "dotenv": "^8.1.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "express-list-endpoints": "^4.0.1",
    "is-ci": "^2.0.0",
    "jest": "^24.8.0",
    "jest-junit": "^8.0.0",
    "maildev": "^1.1.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "objection-gen": "^0.5.2",
    "pino-pretty": "^3.2.0",
    "prettier": "^1.17.1",
    "rimraf": "^3.0.0",
    "string-natural-compare": "^2.0.3",
    "supertest": "^4.0.2",
    "supertest-session": "^4.0.0",
    "surge": "^0.21.3",
    "vuepress": "^1.0.3",
    "wait-on": "^3.3.0"
  }
}
